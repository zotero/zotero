         <single>opus</single>
         <multiple>opera</multiple>
      </term>
      <term name="page">
         <single>página</single>
         <multiple>páginas</multiple>
      </term>
      <term name="paragraph">
         <single>parágrafo</single>
         <multiple>parágrafos</multiple>
      </term>
      <term name="part">
         <single>parte</single>
         <multiple>partes</multiple>
      </term>
      <term name="section">
